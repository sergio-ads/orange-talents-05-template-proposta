package br.com.zupacademy.proposta.consumer.response;

public class CartoesResponse {
	private String id;
	private Long limite;
	
	public CartoesResponse(String id, Long limite) {
		this.id = id;
		this.limite = limite;
	}

	public String getId() {
		return id;
	}

	public Long getLimite() {
		return limite;
	}

}
