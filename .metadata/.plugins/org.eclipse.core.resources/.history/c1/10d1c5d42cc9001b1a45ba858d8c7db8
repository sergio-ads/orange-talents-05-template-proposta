package br.com.zupacademy.proposta.consumer.request;

import br.com.zupacademy.proposta.model.enums.ResultadoAvaliacao;

public class ResultadoAnaliseRequest {

	private String documento;
	private String nome;
	private String resultadoSolicitacao;
	private String idProposta;
	
	public ResultadoAnaliseRequest(String documento, String nome, String resultadoSolicitacao,
			String idProposta) {
		this.documento = documento;
		this.nome = nome;
		this.resultadoSolicitacao = resultadoSolicitacao;
		this.idProposta = idProposta;
	}
	
	
	public String getDocumento() {
		return documento;
	}
	public String getNome() {
		return nome;
	}
	public ResultadoAvaliacao getResultadoSolicitacao() {
		return ResultadoAvaliacao.valueOf(resultadoSolicitacao);
	}
	public String getIdProposta() {
		return idProposta;
	}
	
}
